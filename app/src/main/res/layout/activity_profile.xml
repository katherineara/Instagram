<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="190dp"
        android:onClick="logout"
        android:text="Log out"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="112dp" />

    <ImageView
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="49dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="1dp"
        android:layout_marginStart="0dp"
        app:srcCompat="@drawable/instagram_home_outline_24"
        android:onClick="launchTimeline"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="459dp" />

    <ImageView
        android:id="@+id/create_page"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="4dp"
        android:onClick="launchCreate"
        app:srcCompat="@drawable/instagram_new_post_outline_24"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="459dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="62dp"
        android:layout_height="49dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="3dp"
        app:srcCompat="@drawable/instagram_user_filled_24" />
</RelativeLayout>